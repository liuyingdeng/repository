package cn.xupt.Date;
/**
 * 可视化日历程序
 * @author Administrator
 */
public class Visualcalender {
	public static void main(String[] args) {
		System.out.println("日\t一\t二\t三\t四\t五\t六");
		int j=0;
		for(int i=1;i<=31;i++) {
			System.out.println(i+"\t");
			j++;
			if(j%7==0)
				System.out.print('\n');
		}
	}
}
