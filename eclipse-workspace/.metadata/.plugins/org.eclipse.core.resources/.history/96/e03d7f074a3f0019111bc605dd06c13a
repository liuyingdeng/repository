package cn.xupt.array;
/**
 * @author Administrator
 *
 */
public class Matrix {
	public static void main(String[] args){
		int[][] a= {
				{2,3},
				{4,5},				
		};
		int[][] b= {
				{5,3},
				{3,5},				
		};
		int[][] c=new int[2][2];
	
		for(int i=0;i<c.length;i++)
			for(int j=0;j<c.length;j++) {
			c[i][j]=a[i][j]+b[i][j];
			}
		
		//´òÓ¡¾ØÕó
		for(int i=0;i<c.length;i++) {
			for(int j=0;j<c.length;j++) {
				System.out.print(c[i][j]+"\t");
			}
			System.out.println();
		}
	}

}
